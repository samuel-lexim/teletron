/* Home Slider */
.cms-home #maincontent  .container .main{
    margin-top: -10px;
}
#bannerHS .item:after {
    position: absolute;
    content: '';
    width: 100%;
    height: 100%;
    top: 0;
    right: 0;
}
#bannerHS .carousel-inner .item img{
    width: 100%;
    height: auto;
}
.btHomeSlider {
    position: absolute;
    width: 50px;
    height: 100%;
    top: 0;
    z-index: 0;
}

.prevButtonWrap {
    left: 0;
}

.nextButtonWrap {
    right: 0;
}

.btHomeSlider > span {
    display: block;
    position: relative;
    width: 50px;
    height: 50px;
    top: 50%;
    margin-top: -25px;
    background: rgba(0, 0, 0, 0.3);
}

.btHomeSlider > span:after {
    content: '';
    color: #fff;
    display: inline-block;
    width: 20px;
    height: 20px;
    border-left: 1px solid white;
    border-top: 1px solid white;
    position: relative;
    top: 14px;
}
#bannerHS .nextButtonWrap > span, #bannerHS .prevButtonWrap > span {
    display: none;
    transition: all 0.3s ease-in-out 0s;
}
#bannerHS:hover a .nextButtonWrap >span, #bannerHS:hover a .prevButtonWrap >span{
    display: block;
    transition: all 0.3s ease-in-out 0s;
}

.prevButtonWrap > span:after {
    -ms-transform: rotate(-45deg);
    -webkit-transform: rotate(-45deg);
    transform: rotate(-45deg);
    left: 18px;
}

.nextButtonWrap > span:after {
    -ms-transform: rotate(135deg);
    -webkit-transform: rotate(135deg);
    transform: rotate(135deg);
    left: 12px;
}

.innerHS {
    position: absolute;
    top: 70%;
    left: 18%;
    text-align: center;
    color: #fff;
    z-index: 9;
    -moz-transform: translate(-50%, -50%);
    -webkit-transform: translate(-50%, -50%);
    -o-transform: translate(-50%, -50%);
    -ms-transform: translate(-50%, -50%);
    transform: translate(-50%, -50%);
}
.box_learn_more {
    position: absolute;
    top: 55%;
    left: 50%;
    text-align: center;
    color: #fff;
    z-index: 9;
    -moz-transform: translate(-50%, -50%);
    -webkit-transform: translate(-50%, -50%);
    -o-transform: translate(-50%, -50%);
    -ms-transform: translate(-50%, -50%);
    transform: translate(-50%, -50%);
}
.box_learn_more .learn_more_slider{
    position: relative;
    font-size: 23px;
    margin: 0 auto;
    text-align: center;
    color: #fff;
    font-weight: bold;
    text-decoration: underline;
    letter-spacing: 2px;
}

.innerHS .titHS {
    font-size: 30px;
    margin: 0;
    font-weight: bold;
    line-height: normal;
}

.innerHS .sumHS {
    font-size: 16px;
    margin: 0;
    padding: 20px 0 40px;
}

a.clickHereHS {
    position: relative;
    font-size: 23px;
    display: inline-block;
    margin: 0 auto;
    text-align: center;
    width: 140px;
    /* background: #f05624; */
    padding: 12px 0;
    color: #000;
    border-radius: 5px;
    font-weight: bold;
    text-decoration: underline;
}

@media only screen and (max-width: 767px) {
    .innerHS .titHS {
        font-size: 24px;
    }
}

