   /*  .page-header + .nav-sections {
        margin-bottom: 0;
        background: #F5F5FD;
        position: absolute;
        z-index: 99;
    } */
    .page-header .header .nav-toggle {
        top: 18px;
        display: block;
    }
    .nav-toggle:before{
        display: none;
    }
    .nav-open .nav-sections {
        -webkit-box-shadow: 0 0 5px 0 rgba(50, 50, 50, 0.75);
        -moz-box-shadow: 0 0 5px 0 rgba(50, 50, 50, 0.75);
        -ms-box-shadow: 0 0 5px 0 rgba(50, 50, 50, 0.75);
        box-shadow: 0 0 5px 0 rgba(50, 50, 50, 0.75);
        left: 0;
        z-index: 99;
        width: calc(100% - 400px);
    }
    .nav-sections {
        -webkit-overflow-scrolling: touch;
        -webkit-transition: left 0.3s;
        -moz-transition: left 0.3s;
        -ms-transition: left 0.3s;
        transition: left 0.3s;
        height: 100%;
        left: -80%;
        left: calc(-1 * (100% - 54px));
        overflow: auto;
        position: fixed;
        top: 0;
        width: calc(100% - 54px);
    }
    .nav-before-open .page-wrapper {
        -webkit-transition: left 0.3s;
        -moz-transition: left 0.3s;
        -ms-transition: left 0.3s;
        transition: left 0.3s;
        height: 100%;
        left: 0;
        overflow: hidden;
        /*position: relative;*/
    }
    .nav-open .page-wrapper {
        left: 80%;
        left: calc(100% - 54px);
    }
    #logo-mobile-menu {
        display: block;
    }
    .navbar-scroll {
        display: none;
    }
    .nav-open .nav-sections {
        background: #F1F1F1;
    }
    .show_div_menu {
        overflow: scroll;
        position: fixed;
        top: 0;
        right: 0;
        z-index: 99;
        width: 400px;
        height: 100%;
    }
    .nav-open .navigation > ul > .level0 > .box-child-menu {
        width: 100%;
        display: none;
        position: relative;
        padding: 0;
    }

    .nav-open .navigation > ul > .level0 > .box-child-menu > .ui-corner-all {
        width: 100%;
        position: relative;
    }
    .nav-open .nav-sections-items .nav-sections-item-content .navigation .ui-corner-all > li {
        display: block;
        width: 100%;
    }
    .nav-open .nav-sections-items .nav-sections-item-content .navigation .ui-corner-all > .level1{
        padding-left: 10px;
    }
    .nav-open .nav-sections-items .nav-sections-item-content .navigation .ui-corner-all > .level1 > a{
        font-weight: normal;
        font-size: 12.5px;
    }
    .nav-open .nav-sections-items .nav-sections-item-content {
        padding-left: 0;
    }
    .nav-sections-items .nav-sections-item-content .navigation {
        width: 100%;
    }
    .navigation li.level0 {
        border-top: 1px solid #d1d1d1;
    }
    .nav-open .navigation .level0 > .level-top {
        padding-left: 10px !important;
        /* width: 100%; */
    }
    .html-add-nav {
        display: inline-block;
        float: right;
        margin: 0px;
        padding: 6px 15px;
    }
    .nav-open .navigation .level0.parent:before{
        content: '+';
        display: inline-block;
        float: right;
        margin: 0px;
        padding: 0 20px;
        font-size: 17px;
        color: #404040;
        padding-top: 3px;
    }
    .nav-open .navigation .parent.activeSub:before{
        content: '-' !important;
    }
